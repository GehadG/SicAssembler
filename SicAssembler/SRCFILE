test     start   1000 
         lda     number 
         comp    two 
         jlt     p 
         jeq     p 
         lda     two 
         sta     index 
loop     lda     number 
         sub     one 
         comp    index 
         jeq     p 
         lda     number 
         div     index 
         mul     index 
         sta     num 
         lda     number 
         sub     num 
         comp    zero 
         jeq     exit 
         lda     index 
         add     one 
         sta     index 
         j       0101A 
p        lda     flag 
         add     one 
         sta     flag 
         ldx     zero 
pr       ldch    prime,x 
loop2    td      outdev 
         jeq     loop2 
         wd      outdev 
         tix     five 
         jlt     pr 
         j       finish 
exit     comp    zero 
         ldx     zero 
pri      ldch    notprime,x 
loop3    td      outdev 
         jeq     loop3 
         wd      outdev 
         tix     nine 
         jlt     pri 
number   word    7 
two      word    2 
one      word    1 
index    resw    1 
flag     word    0 
num      resw    1 
zero     word    0 
notprime byte    c'not prime' 
prime    byte    c'prime' 
outdev   byte    x'06' 
five     word    5 
nine     word    9 
finish   end     test 